{
  "name": "Chrome Extension Btn-Click-Event",
  "description": "Estensione che mostra un alert su un click di pulsante",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://localhost:8080/*"],
      "js": ["content.js"]
    }
  ]
}
